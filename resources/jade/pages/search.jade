include ../mixins/grid/mdl-cell

#search
    .mdl-grid.buttons
        +mdl-cell-button('undo', 'turnLeft();')
        +mdl-cell-button('arrow_upward', 'goForward();')
        +mdl-cell-button('redo', 'turnRight();')
    .mdl-grid.buttons
        +mdl-cell-button-half('watch', 'watch();')
        +mdl-cell-button-half('hotel', 'rest();')
        +mdl-cell-button-half('visibility', 'search();')
        +mdl-cell-button-half('pan_tool', 'action();')
        +mdl-cell-button-half('vpn_key', 'useKey();')
        +mdl-cell-button-half('explore', 'compass();')
    .mdl-grid.buttons
        +mdl-cell-button('arrow_backward', 'moveLeft();')
        +mdl-cell-button('arrow_downward', 'turnBack();')
        +mdl-cell-button('arrow_forward', 'moveRight();')
